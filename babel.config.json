{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "edge": "17",
          "firefox": "60",
          "chrome": "67",
          "safari": "11.1"
        },
        "useBuiltIns": "usage",
        "corejs": "3.6.5",
        "shippedProposals": true
      }
    ],
    "@babel/preset-react" //jsx 지원
  ],
  "plugins": [
    "@babel/plugin-transform-runtime" //코드를 재사용하여 codesize를 절약할 수 있는 플러그인입니다.
  ]
}
